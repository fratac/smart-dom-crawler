{
    "name": "fratac/smart-dom-crawler",
    "description": "The smart DOM (the HTML Document Object Model) parser based on Symfony dom-crawler",
    "type": "composer",
    "authors": [
        {
            "name": "fratac",
            "email": "mail@francescotacconi.it"
        }
    ],
    
    "require": {
        "php": ">=8.1",
        "symfony/dom-crawler": "v6.4.0"
    },

    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },

    "autoload": {
        "psr-4": { "SmartDomCrawler\\": "src/" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },


    "minimum-stability": "dev",
    "prefer-stable": true



}
